{
  "name": "gainmap-js",
  "version": "1.0.0",
  "description": "A Javascript (TypeScript) Port of Adobe Gainmap Technology for storing HDR Images using an SDR Image + a gainmap",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./libultrahdr": {
      "import": "./dist/libultrahdr.js",
      "types": "./dist/libultrahdr.d.ts"
    },
    "./wasm": {
      "import": "./libultrahdr-wasm/build/libultrahdr-esm.js"
    },
    "./worker": {
      "import": "./dist/worker.js",
      "types": "./dist/worker.d.ts"
    },
    "./worker-interface": {
      "import": "./dist/worker-interface.js",
      "types": "./dist/worker-interface.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "libultrahdr": [
        "./dist/libultrahdr.d.ts"
      ],
      "worker": [
        "./dist/worker.d.ts"
      ],
      "worker-interface": [
        "./dist/worker-interface.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",

    "test": "jest",
    "pretest": "npm run build",

    "check": "concurrently -c auto npm:check:*",
    "check:typecheck-src": "tsc --noEmit",
    "check:eslint-src": "eslint \"src/**/*.ts\"",
    "check:typecheck-examples": "tsc --noEmit -p examples",
    "check:eslint-examples": "eslint \"examples/**/*.ts\"",
    "check:typecheck-tests": "tsc --noEmit --skipLibCheck -p tests",
    "check:eslint-tests": "eslint \"tests/**/*.ts\"",

    "ci:test": "JEST_JUNIT_OUTPUT_DIR=reports jest --ci --reporters=default --reporters=jest-junit",
    "ci:check": "concurrently npm:ci:check:*",
    "ci:check:typecheck-src": "tsc --pretty false --noEmit > reports/typecheck.log",
    "ci:check:eslint-src": "eslint --format json --output-file reports/eslint-src.json \"src/**/*.ts\"",
    "ci:check:typecheck-examples": "tsc --pretty false --noEmit -p examples > reports/typecheck-examples.log",
    "ci:check:eslint-examples": "eslint --format json --output-file reports/eslint-examples.json \"examples/**/*.ts\"",
    "ci:check:typecheck-tests": "tsc --pretty false --noEmit --skipLibCheck -p tests > reports/typecheck-tests.log",
    "ci:check:eslint-tests": "eslint --format json --output-file reports/eslint-tests.json \"tests/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MONOGRID/gainmap-js.git"
  },
  "keywords": [
    "hdr"
  ],
  "author": "MONOGRID <hello@mono-grid.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MONOGRID/gainmap-js/issues"
  },
  "homepage": "https://github.com/MONOGRID/gainmap-js#readme",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@rollup/plugin-commonjs": "^25.0.5",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.0.0",
    "@semantic-release/git": "github:semantic-release/git",
    "@semantic-release/github": "^9.2.1",
    "@semantic-release/npm": "^11.0.0",
    "@semantic-release/release-notes-generator": "^12.0.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.4",
    "@types/three": "^0.157.0",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.2",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.51.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest-extended": "^2.0.0",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-quasar": "^1.1.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "jest-junit": "^16.0.0",
    "puppeteer": "^21.3.8",
    "rollup": "^2.79.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-license": "^3.1.0",
    "semantic-release": "^22.0.5",
    "three": "^0.157.0",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.2",
    "typedoc-github-wiki-theme": "^1.1.0",
    "typedoc-plugin-markdown": "^3.16.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "three": "^0.157.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "promise-worker-transferable": "^1.0.4"
  }
}
